{
  "name": "handlebars-preview-with-function-support",
  "displayName": "Handlebars Preview with helper functions support",
  "description": "Live preview for Handlebar templates (with support for helper functions)",
  "version": "2.0.8",
  "publisher": "anonimitoraf",
  "engines": {
    "vscode": "^1.5.0"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/anonimitoraf/vscode-handlebars-preview"
  },
  "galleryBanner": {
    "color": "#0F6378",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Handlebars",
    "Live preview",
    "Helper functions"
  ],
  "activationEvents": [
    "onLanguage:handlebars"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "handlebarsPreview.preview",
        "title": "Handlebars: Open Preview"
      }
    ],
    "keybindings": [
      {
        "command": "handlebarsPreview.preview",
        "key": "ctrl+p h",
        "when": "!terminalFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "tsc && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^11.13.7",
    "@types/vscode": "^1.33.0",
    "copyfiles": "^1.2.0",
    "mocha": "^2.3.3",
    "typescript": "^3.4.4",
    "vscode": "^1.1.33"
  },
  "dependencies": {
    "handlebars": "^4.0.6"
  }
}
